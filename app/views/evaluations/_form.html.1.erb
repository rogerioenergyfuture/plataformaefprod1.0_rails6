<script>
$(document).ready(function(){

$("#perg_1").click(function() {
  $("#modal1").addClass("is-active");
});

$("#perg_2").click(function() {
  $("#modal2").addClass("is-active");
});

$("#perg_3").click(function() {
  $("#modal3").addClass("is-active");
});

$(".botao_modal").click(function(event) {
  event.preventDefault();
  $("#modal1,#modal2,#modal3").removeClass("is-active");
});

function favorito(){
   if ($('#evaluation_favorito').val() == "false"){
      $('#evaluation_favorito').val('false');
      $('#btn-favorito').removeClass('fas').addClass('far');
     }else{
      $('#evaluation_favorito').val('true');
      $('#btn-favorito').removeClass('far').addClass('fas');
     }
}

function click_favorito(){
   if ($('#evaluation_favorito').val() == "false"){
      $('#evaluation_favorito').val('true');
      favorito();
     }else{
      $('#evaluation_favorito').val('false');
      favorito();
     }
}

$('#btn-favorito').click(function(){ click_favorito(); });

favorito();

if($("#pontgeral1").text() == ''){
        $("#pontgeral1").text('--');
}

});





